<html lang="en">
<head>
  <title>Challenge page</title>
</head>
<body>
  <%= error %>
  <br/>

  <form method="post" action="/resume">
    <label for="otp">OTP</label>
    <input type="number" name="otp" id="otp">
    <input type="hidden" name="state" value="<%= state %>" >
